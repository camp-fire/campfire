<% include head.ejs%>
<div class="container">
    <div class="row">
      <div class="col-xs-2 col-xs-offset-3 login-icon">
        <img src="images/imac.png" alt="欢迎回到宿舍楼">
        <h4>欢迎回到<small>宿舍楼</small></h4>
      </div>
      <div class="col-xs-5 col-sm-3 login-screen">
        <form action="/reg" method="post" role="form">
          <div class="form-group">
            <input type="text" class="form-control login-field" value="" placeholder="请输入昵称" id="name" name="name" />
          </div>

          <div class="form-group">
            <input type="password" class="form-control login-field" value="" placeholder="请输入密码" id="password" name="password" />
          </div>

          <div class="form-group">
            <input type="password" class="form-control login-field" value="" placeholder="再次输入密码" id="confirmPassword" name="confirmPassword"/>
          </div>

          <button class="btn btn-primary btn-lg btn-block">办  卡</button>
        </form>
      </div>
    </div>
</div>
<% include foot.ejs%>
